c = 'bnjakaa'
d = 'sajakaa'
print(len(c))
print(len(d))
s = len(c)
if ((len(c)%2 == 0) or (len(d)%2 == 0)):
    print('Wyrazy sÄ… parzyste')
else:
    print('Wyrazy sa nieparzyste')
    for i in range(s):
        if c.find(c[i]) == d.find(c[i]):
           print('Jest taka sama litera na miejscu', i, 'I jest to', c[i])
